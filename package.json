{
  "name": "mylistening",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "npm run sync:all",
    "postbuild": "copy dist\\index.html dist\\404.html && type NUL > dist\\.nojekyll",
    "deploy": "gh-pages -d dist -b gh-pages -t",
    "sync:conversation": "node scripts/sync-conversation.mjs",
    "sync:count": "node scripts/update-counts.mjs",
    "sync:all": "npm run sync:conversation && npm run sync:count",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.5.3",
    "eslint": "^9.31.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "~10.3.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.0.12",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4"
  }
}
